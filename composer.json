{
    "name": "ahilmurugesan/skeleton-demo",
    "description": "This is a demo package for user listing",
    "keywords": [
        "ahilmurugesan",
        "skeleton-demo"
    ],
    "homepage": "https://github.com/ahilmurugesan/skeleton-demo",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "ahilan",
            "email": "ahilmurugesan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ahilmurugesan\\SkeletonDemo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahilmurugesan\\SkeletonDemo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahilmurugesan\\SkeletonDemo\\SkeletonDemoServiceProvider"
            ],
            "aliases": {
                "SkeletonDemo": "Ahilmurugesan\\SkeletonDemo\\SkeletonDemoFacade"
            }
        }
    }
}
